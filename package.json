{
  "name": "sn-client-js",
  "version": "1.1.1-development.1",
  "description": "A JavaScript client for Sense/Net ECM that makes it easy to use the REST API of the Content Repository.",
  "main": "index.js",
  "files": [
    "dist",
    "src",
    "documentation",
    "coverage"
  ],
  "scripts": {
    "commit": "git-cz",
    "gulp": "gulp",
    "pretest": "gulp build:clean && npm run build",
    "test": "nyc mocha -p tsconfig.json dist/test/index.js",
    "posttest": "nyc report --reporter=json && nyc report --reporter=lcov",
    "check-coverage": "istanbul check-coverage ",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release pre && semantic-release post",
    "lint": "tslint src/**/*.ts --project tsconfig.json",
    "prebuild": "gulp build:clean",
    "build": "tsc",
    "typedoc:md": "gulp typedoc:md",
    "typedoc:html": "gulp typedoc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SenseNet/sn-client-js.git"
  },
  "keywords": [
    "sensenet",
    "ecm",
    "javascript",
    "REST",
    "ajax",
    "API"
  ],
  "author": "Sense/Net <snteam@sensenet.com> (http://sensenet.com/)",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/SenseNet/sn-client-js/issues"
  },
  "nyc": {
    "exclude": "dist/test/**/*.*"
  },
  "homepage": "https://sensenet.com",
  "dependencies": {
    "@reactivex/rxjs": "5.3.0",
    "app-root-path": "2.0.1",
    "gulp-run": "^1.7.1",
    "nyc": "^10.2.0",
    "ts-json-properties": "1.1.4",
    "typedoc-plugin-external-module-name": "^1.0.9",
    "typescript": "^2.3.0"
  },
  "devDependencies": {
    "@types/app-root-path": "1.2.4",
    "@types/chai": "3.4.35",
    "@types/mocha": "2.2.40",
    "app-root-path": "2.0.1",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "del": "2.2.2",
    "fs-then-native": "2.0.0",
    "gulp": "3.9.1",
    "gulp-header": "1.8.8",
    "gulp-rename": "^1.2.2",
    "gulp-standard": "9.0.0",
    "gulp-tslint": "7.1.0",
    "gulp-typedoc": "2.0.2",
    "istanbul": "0.4.5",
    "jsdoc-to-markdown": "3.0.0",
    "mocha": "3.2.0",
    "mocha-typescript": "^1.0.23",
    "remap-istanbul": "0.9.0",
    "semantic-release": "^6.3.2",
    "ts-json-properties": "1.1.4",
    "tslint": "4.5.1",
    "typedoc": "0.5.10",
    "typedoc-md-theme": "^1.0.1",
    "typescript": "2.3.0"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "typings": "./src/SN.d.ts"
}
